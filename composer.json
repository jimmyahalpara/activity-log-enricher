{
    "name": "jimmyahalpara/laravel-activitylog-enricher",
    "description": "A Laravel package to enrich Spatie ActivityLog entries by resolving foreign key IDs to readable labels",
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "activity-log",
        "spatie",
        "enricher",
        "audit",
        "logging",
        "foreign-keys"
    ],
    "authors": [
        {
            "name": "Jimmy Ahalpara",
            "email": "jimmy@ahalpara.in",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/jimmyahalpara/laravel-activitylog-enricher",
    "support": {
        "issues": "https://github.com/jimmyahalpara/laravel-activitylog-enricher/issues",
        "source": "https://github.com/jimmyahalpara/laravel-activitylog-enricher"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-activitylog": "^4.0",
        "nesbot/carbon": "^2.72.2|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^1.0|^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "JimmyAhalpara\\ActivityLogEnricher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JimmyAhalpara\\ActivityLogEnricher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JimmyAhalpara\\ActivityLogEnricher\\ActivityLogEnricherServiceProvider"
            ],
            "aliases": {
                "ActivityLogEnricher": "JimmyAhalpara\\ActivityLogEnricher\\Facades\\ActivityLogEnricher"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "analyse": "vendor/bin/phpstan analyse",
        "quality": [
            "@format",
            "@analyse",
            "@test"
        ]
    }
}